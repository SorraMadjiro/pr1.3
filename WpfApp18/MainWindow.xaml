        <Menu Grid.Column="1" HorizontalAlignment="Left" Height="35" Margin="453,415,0,0" VerticalAlignment="Top" Width="20" Background="White"/>
        <Menu Grid.Column="1" HorizontalAlignment="Left" Height="50" Margin="503,355,0,0" VerticalAlignment="Top" Width="25" Background="Red"/>
