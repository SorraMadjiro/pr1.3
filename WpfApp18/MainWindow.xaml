        <TextBox x:Name="snilsbox" HorizontalAlignment="Left" Height="23" Margin="80,147,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Column="1"/>
        <Button Content="Проверка" HorizontalAlignment="Left" Margin="35,235,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1" Grid.Column="1"/>
