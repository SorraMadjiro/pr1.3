                <TransformGroup>
                    <ScaleTransform ScaleX="-1"/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Menu.RenderTransform>
        </Menu>
        <Menu Grid.Column="1" HorizontalAlignment="Left" Height="35" Margin="75,415,0,0" VerticalAlignment="Top" Width="21" Background="White" RenderTransformOrigin="0.5,0.5">
